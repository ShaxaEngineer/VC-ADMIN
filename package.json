    "@tanstack/react-query": "^5.51.11",
    "antd": "^5.19.3",
    "apexcharts": "^3.52.0",
    "axios": "^1.7.2",
    "dayjs": "^1.11.12",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
